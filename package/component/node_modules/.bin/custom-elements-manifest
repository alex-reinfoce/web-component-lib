#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules/@custom-elements-manifest/analyzer/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules/@custom-elements-manifest/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules/@custom-elements-manifest/analyzer/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules/@custom-elements-manifest/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/@custom-elements-manifest+analyzer@0.10.3/node_modules:/Users/alex/Project/herm-studio-web-component/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@custom-elements-manifest/analyzer/cem.js" "$@"
else
  exec node  "$basedir/../@custom-elements-manifest/analyzer/cem.js" "$@"
fi
