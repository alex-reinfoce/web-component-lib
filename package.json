{
  "name": "web-component",
  "version": "0.0.1",
  "scripts": {
    "dev:component": "pnpm --filter web-component start",
    "dev:react": "pnpm --filter react-playground dev",
    "dev:vue": "pnpm --filter vue-playground dev",
    "playground": "concurrently \"pnpm run dev:component\" \"pnpm run dev:react\" \"pnpm run dev:vue\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
